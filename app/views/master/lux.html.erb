<script>
    function getScore() {
        setTimeout(() => {
            let element = document.querySelector(":root");
            // add the score value out of 100
            let score = document.querySelector('#score').value;
            if (score <= 100) {
                document.querySelector('#error').classList.add("d-none");
                let angle = (score / 100) * 180;
                element.style.setProperty('--rotate-angle', angle + 'deg');
                element.style.setProperty('--translate-rotate', (angle - 90) + 'deg');
                element.style.setProperty('--translate-x', (angle - 90) + 'px');
                if (score > 50) {
                    element.style.setProperty('--translate-y', (angle - 85) + 'px');
                }
                else if (score < 50) {
                    element.style.setProperty('--translate-y', (90 - angle) + 'px');
                }
                element.style.setProperty('--gradient-score', score + '%');
                document.querySelector("#scoreText").innerHTML = "Tingkat Kebisingan adalah " + score + "dB";
            }
            else if (score > 100) {
                document.querySelector('#error').classList.remove("d-none");
            }
        }, 1000);
    }
</script>